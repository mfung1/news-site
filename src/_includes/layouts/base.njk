<!doctype html>
<html lang="en" class="no-js" data-dark="false">
  <head>
    <meta charset="utf-8">
    <link rel="canonical" href="https://www.lincoln.ac.uk/home/"> 
    <title>{{ pageTitle }}</title>
    <script>
      document.documentElement.className = document.documentElement.className.replace(/\bno-js\b/g, '') + 'has-js ';
    </script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ renderData.description or description or metadata.description }}">
    <meta name="author" content="University of Lincoln">
    <style>.async-hide { opacity: 0 !important} </style>
    {% include "./_styles.njk" %}
  </head>
  <body>
    {% include "./header.njk"%}
    <main id="swup" class="uol-page">
    {{content|safe}}
    </main>
    {% include "./footer.njk"%}
    {% include "./_scripts.njk" %}
  </body>
</html>
